{
  "name": "typescript-template",
  "version": "0.1.0",
  "description": "A Node template using TypeScript and ts-node",
  "main": "index.js",
  "scripts": {
    "start": "node --experimental-strip-types --env-file=.env index.ts",
    "dev": "NODE_ENV=development node --experimental-strip-types --env-file=.env index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AryanJ-NYC/ts-node-typescript-template.git"
  },
  "keywords": [
    "node",
    "typescript",
    "template"
  ],
  "author": "Aryan Jabbari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AryanJ-NYC/ts-node-typescript-template/issues"
  },
  "homepage": "https://github.com/AryanJ-NYC/ts-node-typescript-template#readme",
  "dependencies": {
    "mailgun-js": "^0.22.0",
    "mjml": "^4.8.1",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/mailgun-js": "^0.22.10",
    "@types/mjml": "^4.7.0",
    "@types/node": "^22.0.0",
    "cz-conventional-changelog": "3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
